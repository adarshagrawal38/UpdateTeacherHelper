<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<DialogPane prefHeight="471.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.StudentDialogController">
    <header>
        <AnchorPane prefHeight="43.0" prefWidth="600.0" styleClass="dark-background"
                    stylesheets="@../../layouts/CSS/maincss.css">
            <children>
                <FontAwesomeIcon glyphName="CLOSE" layoutX="559.0" layoutY="34.0" onMouseClicked="#closeEvent"
                                 size="3em" styleClass="icon"/>
            </children>
        </AnchorPane>
    </header>
    <content>
        <VBox fx:id="vbox" prefHeight="200.0" prefWidth="100.0" spacing="20.0" styleClass="light-background"
              stylesheets="@../../layouts/CSS/maincss.css">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
        </VBox>
    </content>
</DialogPane>
